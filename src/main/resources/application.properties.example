spring.application.name=ravenapi
spring.datasource.url=jdbc:mysql:<insert DB url here>
spring.datasource.username=<insert DB username here>
spring.datasource.password=<insert DB user's password here>
api.pagination.buffer.size=<maximum value for each page. value between 1 to 500>
api.character.version.max.last.update.hours=72
api.character.alias.max.last.update.hours=168
server.error.include-stacktrace=never